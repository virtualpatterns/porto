{
  "plugins": [
    "add-module-exports",
    "transform-async-to-generator",
    [
      "nessa/library/transpiler",
      {
        "require": {
          "utilities": "./library/utilities"
        }
      }
    ],
    [
      "transform-runtime",
      {
