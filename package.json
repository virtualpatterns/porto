{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "bin": {
    "porto-server": "./server/bin/porto-server.js"
  },
  "description": "TBD",
  "files": [
    "/deployment",
    "/library",
    "/server",
    "/www/configurations",
    "/www/resources",
    "/www/scripts/bundles",
    "/www/styles",
    "/www/configuration.json",
    "/www/index.html"
  ],
  "license": "ISC",
  "main": "server/index.js",
  "name": "@virtualpatterns/porto",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/porto"
  },
  "scripts": {
    "clean": "rm -r -f ./tasks",
    "build": "npm run-script clean && babel ./source/tasks --copy-files --out-dir ./tasks --quiet --source-maps inline",
    "jake": "npm run-script build && jake --jakefile ./tasks/index.js --trace",
    "tasks": "npm run-script build && jake --jakefile ./tasks/index.js --tasks",
    "upgrade": "npm-check-updates --upgrade",
    "run-debug": "rm -f /var/log/porto/porto.log && npm run-script bundle && node --inspect server/index.js run --databaseUrl mysql://porto:porto@localhost/porto?debug=true --logPath /var/log/porto/porto.log",
    "test-debug": "rm -f /var/log/porto/porto.test.log && npm run-script bundle && npm run-script lint && env ADDRESS=\"0.0.0.0\" DATABASE_URL=\"mysql://porto:porto@localhost/porto?multipleStatements=true\" LOG_PATH=\"/var/log/porto/porto.test.log\" MODULES_PATH=\"./node_modules\" PORT=\"8080\" STATIC_PATH=\"./www\" mocha --bail --inspect-brk --recursive --timeout 0 ./tests",
    "check": "depcheck --ignore-dirs=source,sandbox",
    "get-attendance": "http GET http://localhost:8080/api/attendance",
    "put-attendance": "http PUT http://localhost:8080/api/attendance meetingId=225 userId=71 attended:=true",
    "watch-www": "nodemon --config ./watch/www.json --on-change-only"
  },
  "version": "0.0.33",
  "dependencies": {
    "@material/list": "^0.26.0",
    "@material/toolbar": "^0.25.0",
    "@material/typography": "^0.3.0",
    "@pwn/is": "^1.0.2",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "commander": "^2.12.2",
    "es6-promisify": "^5.0.0",
    "human-format": "^0.9.2",
    "mablung": "0.0.10",
    "moment": "^2.19.3",
    "mysql": "^2.15.0",
    "restify": "^6.3.4",
    "restify-cors-middleware": "^1.1.0",
    "restify-errors": "^5.0.0",
    "restify-plugins": "^1.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-json-schema": "^1.5.0",
    "depcheck": "^0.6.8",
    "eslint": "^4.13.0",
    "faker": "^4.1.0",
    "jake": "^8.0.15",
    "mocha": "^4.0.1",
    "nessa": "0.0.23",
    "nodemon": "^1.12.5",
    "npm-check-updates": "^2.13.0",
    "pm2": "^2.9.1",
    "puppeteer": "^0.13.0",
    "webpack": "^3.10.0"
  }
}
